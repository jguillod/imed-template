{
	"tags": {
		"allowUnknownTags": false,
		"dictionaries": ["jsdoc"]
	},
	"source": {
		"include": [".", "README.md", "package.json", "test/"],
		"includePattern": ".js$",
		"excludePattern": "(imed-template/node_modules/|docs|out|DEPRECATED|_template)"
	},
	"plugins": [
		"plugins/markdown"
	],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": true,
		"useLongnameInNav": false,
		"showInheritedInNav": true
	},
	"opts": {
		"destination": "./docs/",
		"encoding": "utf8",
		"private": true,
		"recurse": true,
		"template": "/usr/local/lib/node_modules/minami",
		"title": "@imed.ch/imed-template"
	}
}
